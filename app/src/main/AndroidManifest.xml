<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.gujaratirasthal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.parth.android.gujaratirasthal.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.parth.android.gujaratirasthal.SabjiActivity"
            android:label="Sabji"/>
        <activity android:name="org.parth.android.gujaratirasthal.ChapatiActivity"
            android:label="Chapati"/>
        <activity android:name="org.parth.android.gujaratirasthal.DrinkActivity"
            android:label="Drink"/>
        <activity android:name="org.parth.android.gujaratirasthal.FarsanActivity"
            android:label="Farsan"/>
        <activity android:name="org.parth.android.gujaratirasthal.FinalOrder"
            android:label="Summary"
            android:noHistory="true"/>
        <activity android:name="org.parth.android.gujaratirasthal.DessertActivity"
            android:label="Dessert">
        </activity>
    </application>

</manifest>